<?xml version="1.0" encoding="UTF-8"?>
<config>
    <propel>
        <datasources default="db_esgiapi">
            <datasource id="db_esgiapi">
                <adapter>mysql</adapter>
                <connection>
                    <dsn>mysql:host=localhost;dbname=db_esgiapi</dsn>
                    <user>esgiapi</user>
                    <password></password>
                    <settings>
                        <setting id="charset">utf8</setting>
                    </settings>
                    <options>
                        <option id="MYSQL_ATTR_INIT_COMMAND">SET NAMES utf8 COLLATE utf8_general_ci</option>
                    </options>
                </connection>
            </datasource>
        </datasources>
    </propel>
</config>